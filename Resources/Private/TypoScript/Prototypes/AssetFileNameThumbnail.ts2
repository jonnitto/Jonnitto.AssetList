prototype(Jonnitto.AssetList:FileNameThumbnail) < prototype(Jonnitto.AssetList:FileNameText) {

    headline.tagName = ${Configuration.setting('Jonnitto.AssetList.thumbnail.headlineTag')}

    thumbnail = TYPO3.TypoScript:Tag {
        @position = 'after headline'
        tagName = 'figure'
        attributes.class = ${namespace + '--figure'}
        content = Jonnitto.AssetList:Image
    }
}
