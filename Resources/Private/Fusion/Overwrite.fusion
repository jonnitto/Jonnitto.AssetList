prototype(Neos.Neos:Page) {
    head.assetList = Neos.Fusion:Tag {
        tagName = 'link'
        attributes {
            rel = 'stylesheet'
            href = Neos.Fusion:ResourceUri {
                path = 'resource://Jonnitto.AssetList/Public/Main.css'
            }
        }
        @if.render = Carbon.Condition:Case {
            enabled = ${Configuration.setting('Jonnitto.AssetList.includeCss')}
            content.nodeType = 'Jonnitto.AssetList:AssetList'
        }
    }
}
