prototype(Jonnitto.AssetList:FileNameText) < prototype(Fusion:Template) {
	templatePath = 'resource://Jonnitto.AssetList/Private/Templates/AssetList.html'
	sectionName = 'FileNameText'

	headline = Fusion:Tag {
		tagName = ${Configuration.setting('Jonnitto.AssetList.text.headlineTag')}
		attributes.class = Fusion:RawArray {
			text = ${namespace + '--headline'}
		}
		content = Jonnitto.AssetList:Title
	}

	action = Jonnitto.AssetList:AssetAction

	@if.hasAsset = ${asset}
}
