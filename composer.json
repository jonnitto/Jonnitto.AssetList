{
    "name": "jonnitto/assetlist",
    "type": "typo3-flow-plugin",
    "license": "MIT",
    "description": "A asset list node type for Neos.io",
    "keywords": ["flow", "neos", "content", "assetlist"],
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "me@jonnitto.ch",
            "homepage": "https://twitter.com/jonnitto",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/neos": "~2.0",
        "carbon/notification": "^0.1"
    },
    "conflict": {
        "jonnitto/nodetypes": "*"
    },
    "archive": {
        "exclude": [".editorconfig", ".gitignore", ".github", ".gitattributes", "gulp.json", "package.json", "CODE_OF_CONDUCT.md"]
    },
    "autoload": {
        "psr-4": {
            "Jonnitto\\AssetList\\": "Classes"
        }
    },
    "extra": {
        "marketplace-account": "me@jonnitto.ch",
        "installer-name": "Jonnitto.AssetList",
        "neos": {
            "package-key": "Jonnitto.AssetList"
        }
    }
}
