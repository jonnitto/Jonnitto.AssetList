{
    "name": "jonnitto/assetlist",
    "type": "neos-plugin",
    "license": "MIT",
    "description": "A asset list node type for Neos.io",
    "keywords": [
        "flow",
        "neos",
        "content",
        "assetlist"
    ],
    "support": {
        "issues": "https://github.com/jonnitto/Jonnitto.AssetList/issues",
        "source": "https://github.com/jonnitto/Jonnitto.AssetList"
    },
    "homepage": "https://github.com/jonnitto/Jonnitto.AssetList",
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "me@jonnitto.ch",
            "homepage": "https://twitter.com/jonnitto",
            "role": "Developer"
        }
    ],
    "require": {
        "neos/neos": "^4.2 || ^5.0",
        "neos/fusion-afx": "^1.2",
        "carbon/notification": "^1.2 || ^2.0",
        "carbon/includeassets": "^3.5 || ^4.0",
        "carbon/eel": "^1.5",
        "carbon/image": "^1.3 || ^2.0 || ^3.0"
    },
    "conflict": {
        "jonnitto/nodetypes": "*"
    },
    "archive": {
        "exclude": [
            ".editorconfig",
            ".eslintignore",
            ".eslintrc",
            ".gitattributes",
            ".gitignore",
            ".github",
            ".jshintrc",
            ".nvmrc",
            ".prettierignore",
            ".prettierrc",
            ".stylelintrc",
            ".yarnclean",
            "CODE_OF_CONDUCT.md",
            "gulp_global.yaml",
            "package.json"
        ]
    },
    "autoload": {
        "psr-4": {
            "Jonnitto\\AssetList\\": "Classes"
        }
    },
    "extra": {
        "marketplace-account": "me@jonnitto.ch",
        "installer-name": "Jonnitto.AssetList",
        "neos": {
            "package-key": "Jonnitto.AssetList"
        }
    }
}
