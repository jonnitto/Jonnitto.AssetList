{
    "name": "jonnitto/assetlist",
    "type": "neos-plugin",
    "license": "GPL-3.0",
    "description": "A asset list node type for Neos.io",
    "keywords": [
        "flow",
        "neos",
        "content",
        "assetlist"
    ],
    "support": {
        "issues": "https://github.com/jonnitto/Jonnitto.AssetList/issues",
        "source": "https://github.com/jonnitto/Jonnitto.AssetList"
    },
    "homepage": "https://github.com/jonnitto/Jonnitto.AssetList",
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "hello@uhlmann.pro",
            "homepage": "https://www.uhlmann.pro",
            "role": "Developer"
        }
    ],
    "require": {
        "neos/neos": "^5.3 || ^7.0 || ^8.0",
        "neos/fusion-afx": "^1.2 || ^7.0 || ^8.0",
        "carbon/notification": "^2.0",
        "carbon/includeassets": "^4.0 || ^5.0",
        "carbon/eel": "^1.5 || ^2.0",
        "carbon/image": "^2.0 || ^3.0"
    },
    "conflict": {
        "jonnitto/nodetypes": "*"
    },
    "archive": {
        "exclude": [
            ".editorconfig",
            ".eslintignore",
            ".eslintrc",
            ".gitattributes",
            ".gitignore",
            ".github",
            ".jshintrc",
            ".nvmrc",
            ".prettierignore",
            ".prettierrc",
            ".stylelintrc",
            "CODE_OF_CONDUCT.md",
            "package.json",
            "pnpm-lock.yaml"
        ]
    },
    "autoload": {
        "psr-4": {
            "Jonnitto\\AssetList\\": "Classes"
        }
    },
    "extra": {
        "installer-name": "Jonnitto.AssetList",
        "neos": {
            "package-key": "Jonnitto.AssetList"
        }
    }
}
